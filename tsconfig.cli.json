{
  "extends": "./tsconfig.json",
  "exclude": ["node_modules", "**/*.fixtures.tsx", "**/*.fixtures.ts", "**/*.test.ts", "**/*.test.tsx"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "noEmit": false,
    "sourceMap": false,
    "importHelpers": true,
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "compiled"
  }
}
