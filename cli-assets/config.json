{
  "name": "jordy default architecture config set",
  "stores": {
    "storeSub": {
      "base": "src/{{withFeature featureName}}/stores/{{subPath}}",
      "excludes": ["core", "common"],
      "files": [
        {
          "fileName": "{{fullName}}.effect.ts",
          "template": "__defTemplates/stores/store-sub-effect.ts.handlebars"
        },
        {
          "fileName": "{{fullName}}.slice.ts",
          "template": "__defTemplates/stores/store-sub-slice.ts.handlebars"
        },
        {
          "fileName": "{{fullName}}.selector.ts",
          "template": "__defTemplates/stores/store-sub-selector.ts.handlebars"
        },
        {
          "fileName": "index.ts",
          "template": "__defTemplates/stores/store-sub-index.ts.handlebars"
        }
      ]
    },
    "storeRoot": {
      "base": "src/{{withFeature featureName}}/stores",
      "excludes": ["core", "common"],
      "files": [
        {
          "fileName": "index.ts",
          "template": "__defTemplates/stores/store-index.ts.handlebars",
          "appendLogic": "appendWithoutDuplicates"
        }
      ]
    },
    "featureRoot": {
      "base": "src/{{withFeature featureName}}",
      "excludes": ["core"],
      "files": [
        {
          "excludes": ["common"],
          "fileName": "reducers.ts",
          "template": "__defTemplates/stores/reducers.ts.handlebars",
          "appendLogic": "appendReducers"
        },
        {
          "fileName": "manipulates/index.ts"
        },
        {
          "fileName": "models/index.ts"
        }
      ],
      "folders": [
        {
          "folderName": "components"
        },
        {
          "folderName": "containers"
        },
        {
          "excludes": ["shared"],
          "folderName": "pages"
        },
        {
          "excludes": ["common", "shared"],
          "folderName": "contexts"
        }
      ]
    },
    "srcRoot": {
      "base": "src",
      "excludes": ["core", "common", "shared"],
      "files": [
        {
          "fileName": "features/feat-reducers.ts",
          "template": "__defTemplates/stores/feat-reducers.ts.handlebars",
          "appendLogic": "appendReducers"
        }
      ]
    }
  },
  "components": {
    "normal": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.tsx",
          "template": "__defTemplates/components/component-normal.tsx.handlebars"
        }
      ]
    },
    "dialog": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.tsx",
          "template": "__defTemplates/components/component-dialog.tsx.handlebars"
        }
      ]
    },
    "imperative": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.tsx",
          "template": "__defTemplates/components/component-imperative.tsx.handlebars"
        }
      ]
    },
    "memo": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.tsx",
          "template": "__defTemplates/components/component-memo.tsx.handlebars"
        }
      ]
    }
  },
  "storybook": {
    "normal": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}/_stories",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.stories.tsx",
          "template": "__defTemplates/components/storybook-normal.stories.tsx.handlebars"
        }
      ]
    },
    "dialog": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}/_stories",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.stories.tsx",
          "template": "__defTemplates/components/storybook-dialog.stories.tsx.handlebars"
        }
      ]
    },
    "imperative": {
      "base": "src/{{withFeature featureName}}/components{{withSubPath subPath}}/_stories",
      "excludes": ["core"],
      "files": [
        {
          "fileName": "{{fileNameAsPascalCase}}.stories.tsx",
          "template": "__defTemplates/components/storybook-imperative.stories.tsx.handlebars"
        }
      ]
    }
  },
  "core": {
    "apiBasePath": ["/api/v3"],
    "openApi": {
      "folderPath": "/Users/theson/Documents/Stoplight Studio/**/",
      "includes": ["v3_admin_banner.yaml", "v3_admin_coordination.yaml"]
    },
    "entity": {
      "base": "src/core/entities/{{featureName}}",
      "files": [
        {
          "fileName": "{{fullName}}.entity.ts",
          "template": "__defTemplates/core/entity-module.ts.handlebars"
        }
      ]
    },
    "references": {
      "order_statuses": {
        "typeName": ""
      }
    },
    "bodySchema": [
      {
        "desc": "common response",
        "typeName": "CommonRes",
        "import": "@core/entities/common",
        "rootEntityPath": "data.payload",
        "pathNeeded": [
          "status",
          "data.meta:$ref=meta_empty$"
        ]
      },
      {
        "desc": "common response (not empty meta data)",
        "typeName": "CommonRes",
        "import": "@core/entities/common",
        "rootEntityPath": "data.payload",
        "pathNeeded": [
          "status",
          "data.meta"
        ]
      },
      {
        "desc": "pageable response",
        "typeName": "PagingRes",
        "import": "@core/entities/common",
        "rootEntityPath": "data.payload[]",
        "pathNeeded": [
          "status",
          "data.meta:$ref=meta_page$"
        ]
      }
    ]
  }
}