import { useMemo } from 'react';
import { Link } from 'react-router-dom';
import { TableColumnsType, Tooltip, Typography } from 'antd';
import cn from 'clsx';

const { Text } = Typography;

export function use{{fileName}}Columns() {
  const result = useMemo(() => {
    const columns: TableColumnsType<SomeUiState> = [
      {
        width: '100px',
        title: '',
        dataIndex: '',
        render: (_, row) => (
          <Tooltip
            mouseLeaveDelay={0}
            title={``}
          >
            <Link
              className="text-blue-500 hover:text-blue-400"
              to={`/${row.blah}`}
            >
              {row.blah}
            </Link>
          </Tooltip>
        ),
      },
    ];

    return columns;
  }, []);

  return result;
}
