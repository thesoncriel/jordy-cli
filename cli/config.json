{
  "module": {
    "name": "ts-fe-toolkit default architecture config set",
    "stores": {
      "storeSub": {
        "base": "src/features/{{featureName}}/stores/{{subName}}",
        "files": [
          {
            "fileName": "{{fullName}}.effect.ts",
            "template": "store-sub-effect.ts.handlebars"
          },
          {
            "fileName": "{{fullName}}.slice.ts",
            "template": "store-sub-slice.ts.handlebars"
          },
          {
            "fileName": "{{fullName}}.selector.ts",
            "template": "store-sub-selector.ts.handlebars"
          },
          {
            "fileName": "index.ts",
            "template": "store-sub-index.ts.handlebars"
          }
        ]
      },
      "storeRoot": {
        "base": "src/features/{{featureName}}/stores",
        "files": [
          {
            "fileName": "index.ts",
            "template": "store-index.ts.handlebars",
            "appendLogic": "simpleAppend"
          }
        ]
      },
      "featureRoot": {
        "base": "src/features/{{featureName}}",
        "files": [
          {
            "fileName": "reducers.ts",
            "template": "reducers.ts.handlebars",
            "appendLogic": "featureReducers"
          }
        ]
      },
      "srcRoot": {
        "base": "src",
        "files": [
          {
            "fileName": "feat-reducers.ts",
            "template": "feat-reducers.ts.handlebars",
            "appendLogic": "srcRootReducers"
          }
        ]
      }
    },
    "components": {
      "normal": {
        "base": "src/features/{{featureName}}/components/{{subName}}",
        "files": [
          {
            "fileName": "{{fileName}}.tsx",
            "template": "component-normal.tsx.handlebars"
          }
        ]
      },
      "dialog": {
        "base": "src/features/{{featureName}}/components/{{subName}}",
        "files": [
          {
            "fileName": "{{fileName}}.tsx",
            "template": "component-dialog.tsx.handlebars"
          }
        ]
      },
      "imperative": {
        "base": "src/features/{{featureName}}/components/{{subName}}",
        "files": [
          {
            "fileName": "{{fileName}}.tsx",
            "template": "component-imperative.tsx.handlebars"
          }
        ]
      },
      "memo": {
        "base": "src/features/{{featureName}}/components/{{subName}}",
        "files": [
          {
            "fileName": "{{fileName}}.tsx",
            "template": "component-memo.tsx.handlebars"
          }
        ]
      }
    },
    "storybook": {
      "normal": {
        "base": "src/features/{{featureName}}/components/{{subName}}/_stories",
        "files": [
          {
            "fileName": "{{fileName}}.stories.tsx",
            "template": "storybook-normal.stories.tsx.handlebars"
          }
        ]
      },
      "dialog": {
        "base": "src/features/{{featureName}}/components/{{subName}}/_stories",
        "files": [
          {
            "fileName": "{{fileName}}.stories.tsx",
            "template": "storybook-dialog.stories.tsx.handlebars"
          }
        ]
      },
      "imperative": {
        "base": "src/features/{{featureName}}/components/{{subName}}/_stories",
        "files": [
          {
            "fileName": "{{fileName}}.stories.tsx",
            "template": "storybook-imperative.stories.tsx.handlebars"
          }
        ]
      }
    }
  }
}